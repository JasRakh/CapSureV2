{
  "expo": {
    "name": "CapSure",
    "slug": "capsure",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.capsure.app"
    },
    "android": {
      "package": "com.capsure.app",
      "permissions": ["CAMERA"]
    },
    "web": {},
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow CapSure to access your camera to scan pills."
        }
      ]
    ],
    "scheme": "capsure"
  }
}
